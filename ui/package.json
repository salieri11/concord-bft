{
  "name": "concord-ui",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run copy-clarity && npm run ng-high-memory -- serve --ssl --proxy-config proxy.conf.json",
    "start:prod": "npm run copy-clarity && ng serve --ssl --proxy-config proxy.conf.json --prod",
    "start:csp": "npm run copy-clarity && ng serve -c csp-dev",
    "build:start:electron": "npm run build:electron  && electron .",
    "start:electron": "electron .",
    "build:prod": "npm run copy-clarity && npm run ng-high-memory -- build --prod",
    "build:electron": "npm run copy-clarity && npm run ng-high-memory -- build -c electron",
    "build:prod:csp": "npm run copy-clarity && npm run ng-high-memory -- build --prod -c csp-production --no-progress",
    "build:local:prod": "ng build --prod --deploy-url assets/ && npm run copy-clarity && npm run build:move",
    "build:local:dev": "ng build --deploy-url assets/ && npm run copy-clarity && npm run build:move",
    "build:move": "mkdir -p ../src/main/resources/static/assets && cp -a dist/. ../src/main/resources/static/assets && mv ../src/main/resources/static/assets/index.html ../src/main/resources/static",
    "copy-clarity": "mkdir -p ./src/static/css && cp ./node_modules/@clr/ui/*.css ./src/static/css",
    "test": "npm run swagmock; ng test --source-map=false",
    "test:build": "npm run swagmock; ng test --source-map=false  --watch=false",
    "test-coverage": "npm run swagmock; ng test --code-coverage",
    "lint": "ng lint concord-ui",
    "lint-fix": "ng lint concord-ui --fix",
    "setup-db": "npm run clear-data-e2e && npm run create-user-e2e && npm run unaccept-agreement",
    "e2e:dev:csp": "npm run swagmock; ng e2e -c csp-dev",
    "e2e:build": "npm run swagmock; npm run webdriver-update && npm run flake-e2e",
    "e2e:dev:integration": "npm run swagmock; ng e2e -c csp-dev-integration",
    "e2e:integration": "npm run swagmock; npm run webdriver-update && npm run flake-e2e:integration",
    "webdriver-update": "webdriver-manager update --versions.chrome=2.46 --gecko false",
    "flake-e2e": "protractor-flake --parser standard  --max-attempts=3 -- protractor.conf.js --baseUrl=https://localhost.vmware.com/",
    "flake-e2e:integration": "protractor-flake --parser standard  --max-attempts=3 -- protractor.integration.conf.js --baseUrl=https://localhost.vmware.com/",
    "clear-data-e2e": "npm run clear-keystores-e2e && npm run clear-contracts-e2e && npm run clear-users-e2e",
    "postinstall": "node patch.js",
    "ng-high-memory": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng",
    "swagmock": "node swagger.mocks.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.7",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^8.2.7",
    "@angular/compiler": "^8.2.7",
    "@angular/core": "^8.2.7",
    "@angular/forms": "^8.2.7",
    "@angular/platform-browser": "^8.2.7",
    "@angular/platform-browser-dynamic": "^8.2.7",
    "@angular/router": "^8.2.7",
    "@auth0/angular-jwt": "^2.1.0",
    "@clr/angular": "^2.2.0",
    "@clr/icons": "^2.2.0",
    "@clr/ui": "^2.2.0",
    "@ngx-translate/core": "~10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@swimlane/ngx-charts": "^11.1.0",
    "@vmw/csp-ngx-components": "8.24.1",
    "@vmw/csp-oauth2": "1.5.0",
    "@vmw/help-panel": "8.2.0",
    "@vmw/ngx-components": "^8.8.9",
    "@vmw/ngx-contextual-help": "^8.0.3",
    "@vmw/ngx-csp-oauth2": "^8.3.1",
    "@vmw/ngx-utils": "^8.4.0",
    "@vmw/ngx-vip": "^7.0.3",
    "@webcomponents/custom-elements": "^1.2.4",
    "bootstrap": "4.0.0-alpha.5",
    "buffer": "^5.2.1",
    "core-js": "^2.4.1",
    "ethereumjs-abi": "0.6.6",
    "geojson-vt": "^3.1.3",
    "json-2-csv": "^3.1.1",
    "jszip": "^3.2.2",
    "keccak": "^3.0.0",
    "lodash": "^4.17.15",
    "lz-string": "^1.4.4",
    "ngx-popper": "4.2.0",
    "ngx-tour-core": "4.0.1",
    "ngx-tour-ngx-popper": "4.0.1",
    "ol": "^5.3.3",
    "popper.js": "^1.14.3",
    "prismjs": "^1.15.0",
    "protractor-flake": "^3.3.0",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.3.3",
    "swagger-ui": "^3.20.0",
    "swaggertooth": "^2.1.9",
    "tslib": "^1.9.0",
    "vivus": "^0.4.4",
    "web3-eth-abi": "^1.2.9",
    "web3-eth-accounts": "^1.2.9",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.5",
    "@angular/cli": "^8.3.5",
    "@angular/compiler-cli": "^8.2.7",
    "@angular/language-service": "^8.2.7",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.144",
    "@types/node": "~6.0.60",
    "@types/vivus": "^0.4.0",
    "ajv": "^6.0.0",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.2",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3"
  }
}
