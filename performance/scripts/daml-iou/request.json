{
  "commands": {
    "commands": [
      {
        "create": {
          "template_id": {
            "package_id": "$package_id",
            "module_name": "$module_name",
            "entity_name": "$entity_name"
          },
          "create_arguments": {
            "fields": [
              {
                "label": "issuer",
                "value": {
                  "party": "$issuer"
                }
              },
              {
                "label": "owner",
                "value": {
                  "party": "$owner"
                }
              },
              {
                "label": "currency",
                "value": {
                  "text": "$currency"
                }
              },
              {
                "label": "amount",
                "value": {
                  "numeric": "$amount"
                }
              },
              {
                "label": "observers",
                "value": {
                  "list": {
                    "elements": [
                      {
                        "party": "$observer"
                      }
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    ],
    "ledger_id": "$ledger_id",
    "workflow_id": "$workflow_id",
    "application_id": "$application_id",
    "command_id": "$command_id",
    "party": "$party"
  }
}