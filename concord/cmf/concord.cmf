Msg GetVersionCommand 2 {
  bytes place_holder
}

Msg DownloadCommand 3 {
  string version
}

Msg WedgeCommand 4 {
  optional int64 stop_seq_num
}

Msg UpgradeCommand 5 {
  string version
}

Msg TestCommand 6 {
  string test
}

Msg ReconfigurationRequest 1 {
  bytes signature
  oneof {
    TestCommand
    GetVersionCommand
    DownloadCommand
    WedgeCommand
    UpgradeCommand
  } command
  bytes additional_data
}
